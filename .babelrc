{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ],
        "root": ["."]
      }
    ],
    [
      "styled-components", {
        "ssr": true
      }
    ]
  ]
}
